{
  "name": "sandpile",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p build-only",
    "build-with-type": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "api-type-to-schema": "typescript-json-schema ./src/api/types/ApiRes.d.ts -o ./src/api/types/ApiRes.json --id res --required=true * & typescript-json-schema ./src/api/types/ApiReq.d.ts -o ./src/api/types/ApiReq.json --id req --required=true *",
    "prepare": "cd .. && husky install client/.husky"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "ajv": "^8.12.0",
    "axios": "^1.2.2",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "element-plus": "^2.3.9",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.0",
    "pinia": "^2.0.28",
    "three": "^0.153.0",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/crypto-js": "^4.1.1",
    "@types/jsdom": "^20.0.1",
    "@types/lodash-es": "^4.17.6",
    "@types/mockjs": "^1.0.7",
    "@types/three": "^0.152.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.3",
    "jsdom": "^20.0.3",
    "less": "^4.1.3",
    "mockjs": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "~5.1.6",
    "typescript-json-schema": "^0.60.0",
    "vite": "^4.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitest": "^0.25.6",
    "vue-tsc": "^1.0.12"
  }
}
